{
  "snippet": {
    "prefix": "snippet",
    "body": [
      "snippet ${1:<trigger>} ${2:<description>}",
      "\t${0:<snippet>}",
      ""
    ]
  },
  "syn": {
    "prefix": "syn",
    "description": "\"syntax snippet of programming language\"",
    "body": [
      "snippet ${1:<trigger>} ${2:<description>}",
      "\tsnippet $1 $2",
      "\t\t\\${0:\\${VISUAL\\}\\}",
      ""
    ]
  },
