{
  "#!": {
    "prefix": "#!",
    "description": "\"shebang\"",
    "body": [
      "#!/bin/csh",
      ""
    ]
  },
  "set": {
    "prefix": "set",
    "description": "\"variable assignment\"",
    "body": [
      "set ${1:<var>} = ${2:<value>}",
      ""
    ]
  },
  "setenv": {
    "prefix": "setenv",
    "description": "\"enviroenment variable assignment\"",
    "body": [
      "setenv ${1:<var>} ${2:<value>}",
      ""
    ]
  },
  "if": {
    "prefix": "if",
    "description": "\"if control flow\"",
    "body": [
      "if( ${1:<condition>} ) then",
      "",
      "\t${0:$TM_SELECTED_TEXT}",
      "",
      "endif",
      ""
    ]
  },
  "ife": {
    "prefix": "ife",
    "description": "\"if else control flow\"",
    "body": [
      "if( ${1:<condition>} ) then",
      "",
      "\t${2:<then branch>}",
      "",
      "else",
      "",
      "\t${3:<else branch>}",
      "",
      "endif",
      ""
    ]
  },
  "fore": {
    "prefix": "fore",
    "description": "\"foreach control flow\"",
    "body": [
      "foreach ${1:<var>} ( ${2:<list>} )",
      "",
      "\t${0:$TM_SELECTED_TEXT}",
      "",
      "end"
    ]
  }
}
